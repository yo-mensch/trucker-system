<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="524.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.SignUpPage">
   <children>
      <Button fx:id="signUpButton" layoutX="216.0" layoutY="429.0" mnemonicParsing="false" onAction="#SignUp" text="Sign up" />
      <TextField fx:id="passwordField" layoutX="71.0" layoutY="91.0" promptText="Password" />
      <TextField fx:id="nameField" layoutX="72.0" layoutY="140.0" promptText="First name" />
      <TextField fx:id="lastnameField" layoutX="72.0" layoutY="189.0" promptText="Last name" />
      <TextField fx:id="phoneField" layoutX="71.0" layoutY="237.0" promptText="Phone number" />
      <DatePicker fx:id="birthField" layoutX="60.0" layoutY="336.0" promptText="Date of birth" />
      <TextField fx:id="emailField" layoutX="72.0" layoutY="286.0" promptText="Email" />
      <CheckBox fx:id="adminField" layoutX="301.0" layoutY="278.0" mnemonicParsing="false" text="I am an admin" />
      <CheckBox fx:id="medicalCheck" layoutX="276.0" layoutY="95.0" mnemonicParsing="false" text="Have valid medical card" />
      <CheckBox fx:id="managerCheck" layoutX="111.0" layoutY="391.0" mnemonicParsing="false" onAction="#disableFields" text="Manager" />
      <TextField fx:id="distanceField" layoutX="275.0" layoutY="189.0" promptText="Full driven distance" />
      <CheckBox fx:id="licenceCheck" layoutX="275.0" layoutY="144.0" mnemonicParsing="false" text="Have valid driver licence" />
   </children>
</AnchorPane>
